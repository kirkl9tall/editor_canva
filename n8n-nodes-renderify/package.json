{
  "name": "n8n-nodes-renderify",
  "version": "0.1.0",
  "description": "n8n community node for Renderify image generation API",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "https://github.com/your-org/n8n-nodes-renderify",
  "author": {
    "name": "Renderify"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "dev": "tsc --project tsconfig.json --watch",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/RenderifyApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Renderify/Renderify.node.js"
    ]
  },
  "devDependencies": {
    "n8n-workflow": "*",
    "typescript": "^5"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
